{
  "name": "electro-stack",
  "version": "0.1.0",
  "private": true,
  "description": "Remix Stack based on MUI and serverless supabase (DB & Auth)",
  "keywords": [
    "Remix",
    "Supabase",
    "Vercel"
  ],
  "author": "RaphaÃ«l Morineau <raphael.morineau@gmail.com>",
  "maintainers": [],
  "license": "MIT",
  "repository": "https://github.com/Sharlaan/electro-stack",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "postinstall": "remix setup node",
    "lint": "eslint app --fix",
    "prepare": "husky install"
  },
  "engines": {
    "node": "14"
  },
  "dependencies": {
    "@remix-run/node": "latest",
    "@remix-run/react": "latest",
    "@remix-run/vercel": "latest",
    "@supabase/supabase-js": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "latest",
    "react-use": "latest"
  },
  "devDependencies": {
    "@remix-run/dev": "latest",
    "@remix-run/eslint-config": "latest",
    "@remix-run/serve": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "typescript": "latest"
  },
  "sideEffects": false,
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|mjs|ts|tsx|css|md|yml)": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "14.19.3"
  }
}
